<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="143.0" prefWidth="236.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.view.signIn">
    <TextField fx:id="emailField" layoutX="20.0" layoutY="20.0" prefHeight="34.0" prefWidth="216.0" promptText="Email">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </VBox.margin>
    </TextField>
    <PasswordField fx:id="passwordField" prefHeight="36.0" prefWidth="216.0" promptText="Password">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
        </VBox.margin></PasswordField>
    <Button onAction="#signInButton" prefHeight="32.0" prefWidth="216.0" text="Sign In">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
        </VBox.margin></Button>
</VBox>
